{
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": "tsconfig.json"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "HEAD",
                "OPTIONS"
            ]
        }
    ]
}